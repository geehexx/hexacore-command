{
  "extensions": {
    "front-matter": {
      "enabled": true
    }
  },
  "plugins": {
    "md013": {
      "line_length": 250,
      "heading_line_length": 250,
      "code_block_line_length": 250
    },
    "md041": {
      "enabled": false
    },
    "custom": [
      {
        "id": "front-matter-structure",
        "module-path": "tools/pymarkdown_plugins/front_matter_validator.py"
      }
    ]
  }
}
